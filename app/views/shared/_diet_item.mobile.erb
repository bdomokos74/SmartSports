<li>
  <a id="diet_item_<%= diet_item.id %>" href="#editFoodPage" data-foodid="<%= diet_item.id %>" data-transition="slidefade">
    <%= image_tag "#{diet_item.type.downcase}40.png" %>
    <% if diet_item.type != "Smoke" %>
      <h2><%= diet_item.food_type.name if diet_item.food_type %></h2>
      <p><%= diet_item.date %></p>
    <% else %>
      <h2><%= diet_item.name %></h2>
      <p><%= diet_item.date %></p>
    <% end %>
  </a>

</li>
