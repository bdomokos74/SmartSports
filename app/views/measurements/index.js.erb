<% if not @is_mobile %>
    $("#recentMeasTable").html("");
    $("#recentMeasTable").html("<%= escape_javascript(render :partial=> 'shared/meas_item', :collection=> @measurements , :locals => {:meas_map => @meas_map }) %>");
<% else %>
    console.log("load measurements js mobile called");
    $("#healthPage .recentResourcesListview").html("");
    $("#healthPage .recentResourcesListview").html("<%= escape_javascript( render partial: "shared/meas_item", formats: [:mobile], collection: @measurements, as: "meas_item", :locals => {:meas_map => @meas_map }) %>");
    $("#healthPage .recentResourcesListview").listview( "refresh" );
    $("#healthPage .recentResourcesListview").trigger( "updatelayout");
<% end %>