<table>
    <thead>
    <tr>
        <th>User name</th>
        <th>Source</th>
        <th>Relative</th>
        <th>Disease</th>
        <th>Note</th>
        <th>Created at</th>
        <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @family_histories.each do |family_hist| %>
        <tr>
            <td><%= family_hist.user.name %></td>
            <td><%= family_hist.source %></td>
            <td><%= family_hist.relative %></td>
            <td><%= family_hist.disease %></td>
            <td><%= family_hist.note %></td>
            <td><%= family_hist.created_at %></td>
        </tr>
    <% end %>
    </tbody>
</table>

<br>

