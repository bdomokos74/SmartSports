<%= render partial: 'static_data' %>

<%= render "patient_select" %>

<div id="sectionPatients" class="menu-section">
  <%= render "patients" %>
</div>

<div id="openModal" class="modalDialog">
  <div>
    <div class="table-toolbar">
      <span id="close-patient-data" class="right page-control modalClose"><i class="fa fa-times fa-2x"></i></span>
      <span id="download-patient-data" class="right page-control"><i class="fa fa-download fa-2x"></i></span>
    </div>
    <br/>
    <br/>
    <div id="patient-data-container">
      <table id="patient-data" class="display" cellspacing="0" width="100%"></table>
    </div>
  </div>
</div>

<% content_for :menu do %>
    <%= render "mdmenu" %>
<% end %>
<% content_for :mobile_menu do %>
    <%= render "mobile_mdmenu" %>
<% end %>

<script type="text/javascript">
    mdPatientsLoaded();
</script>