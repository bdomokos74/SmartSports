    <div class="columns">

        <div id="heart-trend-container" class="boxed-group connection-block">

            <h3 class="noClickSelect">
                <img src="<%= asset_path( "withings-logo.png")%>" width="24px" height="24px" class="left">
                <div class="training-type left">Heart trend</div>
                <div class="selected-meas legend-container right"></div>
            </h3>

            <div class="boxed-group-inner chart bloodpressure-chart">
                <svg class="heart-trend-chart-svg">
                </svg>
            </div>
        </div>
    </div>

    <div class="columns">

        <div id="bloodsugar-container" class="boxed-group connection-block">

            <h3 class="noClickSelect">
                <img src="<%= asset_path( "logo-login.png")%>" width="24px" height="24px" class="left">
                <div class="training-type left">Blood glucose level</div>
                <div class="selected-meas left"></div>
                <div class="selected-meas legend-container right"></div>
            </h3>

            <div class="boxed-group-inner chart bloodsugar-chart">
                <svg class="bloodsugar-chart-svg">
                </svg>
            </div>
        </div>

    </div>

    <div class="columns">
        <div id="activity-trend-container" class="boxed-group">
            <h3 class="noClickSelect">
                <img src="<%= asset_path( "logo-login.png")%>" width="24px" height="24px" class="left">
                <div class="training-type left">Activity trend</div>
                <div class="selected-meas legend-container right"></div>
            </h3>

            <div class="boxed-group-inner chart activity-trend-chart">
                <svg class="activity-trend-chart-svg">
                </svg>
            </div>
        </div>
    </div>

    <div class="columns">
        <div class="column one-half">

            <div id="daily-piechart-container" class="boxed-group">
                <h3 class="noClickSelect">
                    <img src="<%= asset_path( "logo-login.png")%>" width="24px" height="24px" class="left">
                    <div class="training-type left">Time tracker</div>
                    <div class="notes left"></div>
                    <div class="selected-meas legend-container right"></div>
                </h3>

                <div class="boxed-group-inner chart daily-piechart">
                    <svg class="pie-chart">
                    </svg>
                    <div class="pie-label">
                        <span class="pie1-label">2015-02-01</span>
                        <span class="pie2-label">Weekly</span>
                    </div>
                </div>

            </div>

        </div>

        <div class="column one-half">

        </div>
    </div>

    <div id="legend-template" class="hidden">
        <span class="chart-legend"></span>
    </div>

</div>

<% content_for :menu do %>
    <%= render "menu" %>
<% end %>
<% content_for :page_logic do %>
    analytics_loaded()
<% end %>
<% content_for :title_text do %>
    Analitics | SmartDiab
<% end %>
