<div class="explore-block" id="explore-container">
    <div class="columns">
        <div id="explore-trend-container" class="boxed-group">
            <h3 class="noClickSelect">
                <img src="<%= asset_path( "logo-login.png")%>" width="24px" height="24px" class="left">
                <div class="training-type left">Activity times</div>
                <div class="notes left"></div>
                <div class="selected-meas legend-container right"></div>
            </h3>

            <div class="boxed-group-inner chart activity-trend-chart">
                <svg class="explore-trend-chart-svg">
                </svg>
            </div>
        </div>
    </div>
    <div class="columns">
        <div class="column one-half">

            <div id="daily-piechart-container" class="boxed-group">
                <h3 class="noClickSelect">
                    <img src="<%= asset_path( "logo-login.png")%>" width="24px" height="24px" class="left">
                    <div class="training-type left">Time tracker</div>
                    <div class="notes left"></div>
                    <div class="selected-meas legend-container right"></div>
                </h3>

                <div class="boxed-group-inner chart daily-piechart">
                    <svg class="pie-chart">
                    </svg>
                    <div class="pie-label">
                        <span class="pie1-label">2015-02-01</span>
                        <span class="pie2-label">Weekly</span>
                    </div>
                </div>

            </div>

        </div>

        <div class="column one-half">

        </div>
    </div>

    <div id="legend-template" class="hidden">
        <span class="chart-legend"></span>
    </div>
</div>

<% content_for :page_logic do %>
    wellbeing_loaded()
<% end %>
<% content_for :title_text do %>
    Well-being | SmartDiab
<% end %>